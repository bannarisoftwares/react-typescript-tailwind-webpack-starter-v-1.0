{
  "name": "react-typescript-tailwind-webpack-starter",
  "version": "1.0.0",
  "scripts": {
    "dev": "webpack serve --hot",
    "build": "NODE_ENV=production webpack",
    "prebase": "postcss --config src/css/base src/css/base/*.css --base src/css --dir dist",
    "base": "cat dist/base/*.css > dist/base.css",
    "postbase": "prejss-cli dist/base.css --format commonjs",
    "preutilities:global": "postcss --config src/css/utilities/global src/css/utilities/global/*.css --base src/css --dir dist",
    "utilities:global": "cat dist/utilities/global/*.css > dist/utilities.css",
    "postutilities:global": "prejss-cli dist/utilities.css --format commonjs",
    "preutilities:unstyled": "postcss --config src/css/utilities/unstyled src/css/utilities/unstyled/*.css --base src/css --dir dist",
    "utilities:unstyled": "cat dist/utilities/unstyled/*.css > dist/utilities-unstyled.css",
    "postutilities:unstyled": "prejss-cli dist/utilities-unstyled.css --format commonjs",
    "preutilities:styled": "postcss --config src/css/utilities/styled src/css/utilities/styled/*.css --base src/css --dir dist",
    "utilities:styled": "cat dist/utilities/styled/*.css > dist/utilities-styled.css",
    "postutilities:styled": "prejss-cli dist/utilities-styled.css --format commonjs",
    "components": "postcss --config src/css/components src/css/components/**/*.css --base src/css --dir dist",
    "merge:unstyled": "cat dist/components/unstyled/*.css > dist/unstyled.css",
    "merge:styled": "cat dist/components/unstyled/*.css dist/components/styled/*.css > dist/styled.css",
    "rtl": "rtlcss dist/unstyled.css -s && rtlcss dist/styled.css -s",
    "prejss": "prejss-cli dist/{unstyled,styled}{,.rtl}.css --format commonjs",
    "themes": "postcss src/css/themes/index.css -o dist/themes.css --config src/css/themes",
    "full": "postcss src/css/full/index.css -o dist/full.css --config src/css/full",
    "postfull": "parcel-css --minify --targets '> 7%' dist/full.css -o dist/full.css",
    "buildui": "npm run base && npm run utilities:global && npm run utilities:unstyled && npm run utilities:styled && npm run components && npm run merge:unstyled && npm run merge:styled && npm run rtl && npm run prejss && npm run themes && npm run full",
    "install:docs": "cd src/css/docs && npm install",
    "add": "touch src/css/components/unstyled/$npm_config_name.css && touch src/css/components/styled/$npm_config_name.css",
    "postadd": "open src/css/components/unstyled/$npm_config_name.css && open src/css/components/styled/$npm_config_name.css",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@tailwindcss/jit": "^0.1.18",
    "@types/react": "17.0.27",
    "@types/react-dom": "17.0.9",
    "@types/tailwindcss": "2.2.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.4.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "postcss": "^8.3.9",
    "postcss-loader": "^6.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "tailwindcss": "^2.2.16",
    "webpack": "^5.58.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "mini-css-extract-plugin": "^2.6.0",
    "react-refresh": "^0.11.0",
    "terser-webpack-plugin": "^5.3.1",
    "@parcel/css-cli": "^1.5.0",
    "@types/css-selector-tokenizer": "^0.7.1",
    "autoprefixer": "10.0.4",
    "postcss-cli": "8.3.0",
    "postcss-import": "13.0.0",
    "postcss-nested": "5.0.1",
    "prejss-cli": "0.3.3",
    "rtlcss": "3.0.0"
  },
  "dependencies": {
    "daisyui": "^2.13.3"
  }
}
